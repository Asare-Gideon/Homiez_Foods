{
  "expo": {
    "name": "Homiez_Food",
    "slug": "Homiez_Food",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ea580c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "/assets/logo-no-bg.png",
          "color": "#ffffff",
          "sounds": ["./assets/sound/new-order-alert.mp3"]
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC-tLlsTucUW3ZDVqHJBfImnepvPttEOrw"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ea580c"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
